; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

[application]

config/name="KirboidVania"
config/description="I have no idea."
run/main_scene="res://scenes/menus/titleScreen.tscn"
run/delta_sync_after_draw=true
boot_splash/show_image=false
config/icon="res://assets/images/icon.png"

[autoload]

DataHandler="*res://assets/scripts/utilities/DATA_HANDLER.tscn"

[debug]

gdscript/warnings/return_value_discarded=false

[display]

window/size/width=1280
window/size/height=720
window/size/fullscreen=true
window/vsync/vsync_via_compositor=true

[global]

vsync=false

[gui]

common/drop_mouse_on_gui_input_disabled=true

[layer_names]

2d_physics/layer_1="player"
2d_physics/layer_2="ground"
2d_physics/layer_3="items"
2d_physics/layer_4="enemies"
2d_physics/layer_5="hurtboxes"

[physics]

2d/thread_model=2
common/physics_interpolation=true

[rendering]

quality/driver/fallback_to_gles2=true
2d/snapping/use_gpu_pixel_snap=true
environment/default_environment="res://assets/default_env.tres"
quality/dynamic_fonts/use_oversampling=false
